# Production environment variables for Docker Compose
# Copy this file to .env.prod and fill in your values

# Database credentials
DB_USER=postgres
DB_PASSWORD=secure_password_here

# Telegram configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_GROUP_ID=your_group_id_here
MINI_APP_URL=https://your-app-url.example.com

# Optional: override Bunq API base (defaults to https://api.bunq.com/v1)
# For sandbox testing you might set it to https://public-api.sandbox.bunq.com/v1
BUNQ_API_URL=

# JWT secret for signing auth cookies
JWT_SECRET=

TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_MESSAGING_SERVICE_SID=
